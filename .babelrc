{
    "presets": ["env", "react"],
    "plugins": [
        ["module-resolver", {
            "root": ["./src"],
            "alias": {
                "@components": "./src/components",
                "@containers": "./src/containers",
                "@assets": "./src/assets",
                "@utils": "./src/utils",
                "@store": "./src/store"
            }
        }]
    ]
}